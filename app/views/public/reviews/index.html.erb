<div class='col-md-8 offset-md-1'>
<h1>Public::Reviews#index</h1>
<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th></th>
      <th>Title</th>
      <th>Opinion</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @reviews.each do |review| %>
      <tr>
        <td><%= link_to review.title,review %></td>
        <td><%= review.body %></td>

      </tr>
    <% end %>
  </tbody>
</table>
</div>

